{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "Nom"
    },
    "Nom": {
      "type": "string"
    },
    "pageBackgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "showMenu": {
      "type": "boolean"
    },
    "showFooter": {
      "type": "boolean"
    },
    "ellipses": {
      "displayName": "ellipse",
      "type": "component",
      "repeatable": true,
      "component": "c4d-components.ellipse"
    }
  }
}
